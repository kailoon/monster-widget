get_text	monster-widget.php	/^	public function get_text() {$/;"	f
get_widget_class	monster-widget.php	/^	public function get_widget_class( $widget ) {$/;"	f
get_widget_config	monster-widget.php	/^	public function get_widget_config() {$/;"	f
html	monster-widget.php	/^		$html = array();$/;"	v
html	monster-widget.php	/^		$html = implode( "\\n", $html );$/;"	v
id	monster-widget.php	/^			$id = $args['id'];$/;"	v
iterator	monster-widget.php	/^	static $iterator = 1;$/;"	v
menu	monster-widget.php	/^		$menu = array_shift( $menus );$/;"	v
menu	monster-widget.php	/^		if ( $menu = $this->get_nav_menu() ) {$/;"	v
menus	monster-widget.php	/^		$menus = array_combine( $counts, $menus );$/;"	v
menus	monster-widget.php	/^		$menus = array_reverse( $menus );$/;"	v
menus	monster-widget.php	/^		$menus = array_values( $menus );$/;"	v
menus	monster-widget.php	/^		$menus = wp_get_nav_menus();$/;"	v
name	monster-widget.php	/^	 * @return string Dynamic class name a given widget.$/;"	c
register_monster_widget	monster-widget.php	/^function register_monster_widget() {$/;"	f
widget	monster-widget.php	/^	public function widget( $args, $instance ) {$/;"	f
